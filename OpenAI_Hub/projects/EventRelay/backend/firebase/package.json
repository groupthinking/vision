{
  "name": "firebase-project",
  "version": "1.0.0",
  "description": "Firebase project with comprehensive documentation system",
  "main": "index.js",
  "scripts": {
    "docs:validate": "node scripts/docs-maintenance.js --validate",
    "docs:update": "node scripts/docs-maintenance.js",
    "docs:create-templates": "node scripts/create-doc-templates.js",
    "docs:check-links": "node scripts/docs-maintenance.js --validate | grep broken_link",
    "docs:daily-update": "node scripts/docs-maintenance.js",
    "docs:weekly-review": "node scripts/docs-maintenance.js --validate && node scripts/generate-weekly-report.js",
    "docs:monthly-audit": "node scripts/docs-maintenance.js && node scripts/generate-monthly-report.js",
    "docs:cleanup": "node scripts/cleanup-duplicates.js",
    "docs:update-refs": "node scripts/update-cross-references.js",
    "docs:report": "node scripts/generate-maintenance-report.js",
    "pm:init": "node integrations/project-management/pm-cli.js init",
    "pm:sync": "node integrations/project-management/pm-cli.js sync",
    "pm:create": "node integrations/project-management/pm-cli.js create",
    "pm:status": "node integrations/project-management/pm-cli.js status",
    "pm:webhook": "node integrations/project-management/pm-cli.js webhook",
    "pm:config": "node integrations/project-management/pm-cli.js config",
    "pm:test": "node integrations/project-management/pm-cli.js test",
    "pm:server": "node integrations/project-management/webhook-server.js",
    "security:scan": "node security/security-scan.js",
    "security:audit": "npm audit --audit-level moderate",
    "security:lint": "eslint src/ --ext .js,.ts --config security/.eslintrc.security.js",
    "security:dependency-check": "dependency-check.sh --project 'Firebase Project' --scan . --out reports/ --format HTML",
    "security:container-scan": "trivy image --format json --output reports/trivy-results.json",
    "security:report": "node security/security-scan.js --report",
    "perf:test": "artillery run tests/performance/load-test.yml",
    "perf:analyze": "node scripts/performance-analyzer.js",
    "perf:compare": "node scripts/performance-comparator.js",
    "perf:baseline": "node scripts/performance-baseline.js",
    "monitoring:start": "node monitoring/monitoring-cli.js start",
    "monitoring:stop": "node monitoring/monitoring-cli.js stop",
    "monitoring:status": "node monitoring/monitoring-cli.js status",
    "monitoring:metrics": "node monitoring/monitoring-cli.js metrics",
    "monitoring:alerts": "node monitoring/monitoring-cli.js alerts",
    "monitoring:report": "node monitoring/monitoring-cli.js report",
    "monitoring:baseline": "node monitoring/monitoring-cli.js baseline",
    "monitoring:config": "node monitoring/monitoring-cli.js config",
    "monitoring:test": "node monitoring/monitoring-cli.js test",
    "analytics:start": "node analytics/analytics-engine.js start",
    "analytics:status": "node analytics/analytics-engine.js status",
    "analytics:report": "node analytics/analytics-engine.js report",
    "ai-docs:generate": "node ai-docs/ai-documentation-generator.js generate",
    "ai-docs:analyze": "node ai-docs/ai-documentation-generator.js analyze",
    "compliance:check": "node compliance/compliance-checker.js check",
    "compliance:report": "node compliance/compliance-checker.js report",
    "compliance:status": "node compliance/compliance-checker.js status",
    "risk:start": "node risk-assessment/predictive-risk-assessor.js start",
    "risk:predict": "node risk-assessment/predictive-risk-assessor.js predict",
    "risk:report": "node risk-assessment/predictive-risk-assessor.js report",
    "risk:status": "node risk-assessment/predictive-risk-assessor.js status",
    "docker:build": "docker build -t firebase-app .",
    "docker:run": "docker run -p 3000:3000 firebase-app",
    "docker:compose": "docker-compose up -d",
    "k8s:deploy": "kubectl apply -f kubernetes/",
    "k8s:status": "kubectl get pods,services,ingress",
    "db:monitor": "node database-monitoring/db-monitor.js start",
    "db:status": "node database-monitoring/db-monitor.js status",
    "db:report": "node database-monitoring/db-monitor.js report",
    "notify:test": "node communications/notification-manager.js test",
    "notify:status": "node communications/notification-manager.js status",
    "gitlab:webhook": "node integrations/project-management/pm-cli.js webhook start",
    "gitlab:status": "node integrations/project-management/pm-cli.js webhook status",
    "external:start": "node external-monitoring/service-monitor.js start",
    "external:status": "node external-monitoring/service-monitor.js status",
    "gitlab:setup-webhooks": "node scripts/setup-gitlab-webhooks.js setup",
    "gitlab:auto-setup": "node scripts/setup-gitlab-webhooks.js auto",
    "gitlab:validate-webhooks": "node scripts/setup-gitlab-webhooks.js validate",
    "gitlab:list-webhooks": "node scripts/setup-gitlab-webhooks.js list",
    "gitlab:token-instructions": "node scripts/setup-gitlab-webhooks.js token"
  },
  "keywords": [
    "firebase",
    "documentation",
    "maintenance",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.4.0"
  }
}
