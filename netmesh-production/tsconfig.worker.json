{
	"extends": "./tsconfig.node.json",
	"compilerOptions": {
		"composite": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.worker.tsbuildinfo",
		"types": [
			"./worker-configuration.d.ts",
			"vite/client",
			"jest"
		],
		"lib": [
			"ES2023"
		],
		"baseUrl": ".",
		"paths": {
			"shared/*": [
				"./shared/*"
			],
			"worker/*": [
				"./worker/*"
			]
		}
	},
	"include": [
		"./worker-configuration.d.ts",
		"./shared",
		"./worker"
	]
}