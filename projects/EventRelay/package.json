{
  "name": "eventrelay",
  "version": "2.0.0",
  "private": true,
  "packageManager": "npm@10.8.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "build:frontend": "cd frontend && npm run build",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd frontend && npm install"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "frontend"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.24.1",
    "turbo": "^2.0.0"
  },
  "overrides": {
    "nth-check": "^2.0.1",
    "postcss": "^8.4.31",
    "undici": "^6.22.0"
  },
  "dependencies": {
    "@langchain/core": "^0.3.77",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/openai": "^0.6.13",
    "chrome-devtools-mcp": "^0.10.2"
  }
}
